{
    "name": "eold/yii2-apidoc-generator",
    "type": "yii2-extension",
    "description": "A simple comments generator in Apidoc.js format for your yii2 RESTFul API's",
    "keywords": ["yii2", "apidoc", "apidoc.js", "generator"],
    "license": "BSD",
    "support": {
        "issues": "https://github.com/enryold/yii2-apidoc-generator"
    },
    "authors": [
        {
            "name": "Enrico Vecchio",
            "email": "enrico.vecchio1@gmail.com"
        }
    ],

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/enryold/yii2-apidoc-generator"
        }
    ],

    "require-dev": {
        "yiisoft/yii2": "*"
    },

    "autoload": {
        "psr-4": { "eold\\apidocgen\\": "" }
    }
}
